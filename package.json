{
  "name": "mqtt2tivoremote",
  "version": "1.0.0",
  "description": "Make TiVo DVR remote control available through an mqtt-smarthome style interface.",
  "bin": {
    "mqtt2tivoremote": "dist/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src -d dist -D",
    "clean": "rm -rf dist/",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forty2/mqtt2tivoremote.git"
  },
  "bugs": {
    "url": "https://github.com/forty2/mqtt2tivoremote/issues"
  },
  "homepage": "https://github.com/forty2/mqtt2tivoremote#readme",
  "author": "Zach Bean",
  "license": "MIT",
  "keywords": [
    "TiVo",
    "DVR",
    "remote",
    "control",
    "home",
    "automation",
    "mqtt",
    "smarthome"
  ],
  "dependencies": {
    "mqtt": "^1.14.1",
    "rxjs": "^5.3.0",
    "source-map-support": "^0.4.15",
    "tivo-remote": "^1.0.0",
    "yalm": "^4.0.2",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "babel-plugin-syntax-function-bind": "^6.13.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-env": "^1.4.0"
  },
  "engines": {
    "node": ">=6.0"
  },
  "babel": {
    "sourceMaps": "inline",
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 6
          }
        }
      ]
    ],
    "plugins": [
      "syntax-function-bind",
      "transform-function-bind",
      "transform-object-rest-spread"
    ]
  }
}
